html{box-sizing:border-box;-webkit-text-size-adjust:none}*,*:before,*:after{box-sizing:inherit}div,article,section,main,footer,header,form,fieldset,pre,code,p,ul,ol,li,dl,dt,dd,textarea,input,label,select{margin:0 0 .75em}*:first-child{margin-top:0}*:last-child{margin-bottom:0}button{border:none;margin:0;padding:0}legend{padding:0}html,body{height:100%;margin:0;padding:0;width:100%}html{font-size:16px;text-size-adjust:none}body{background-color:rgba(34,34,34,0.05);color:#222;font-family:-apple-system,BlinkMacSystemFont,"Segoe UI","Roboto","Oxygen","Ubuntu","Cantarell","Fira Sans","Droid Sans","Helvetica Neue",sans-serif;font-size:.875em;line-height:1.5;margin:0;padding:0;-webkit-font-smoothing:antialiased;-moz-osx-font-smoothing:grayscale}main{margin:auto;padding:2rem}hr{border-style:solid;border-width:0;border-top-width:1px;color:rgba(34,34,34,0.1);margin-bottom:2em;margin-top:2em}.flex{display:flex;flex-flow:row wrap}.flex--0{flex:0}.flex--1{flex:1}.flex--2{flex:2}.flex--3{flex:3}.flex-shrink--0{flex-shrink:0}.flex-shrink--1{flex-shrink:1}.flex-shrink--2{flex-shrink:2}.flex-shrink--3{flex-shrink:3}.flex-grow--0{flex-grow:0}.flex-grow--1{flex-grow:1}.flex-grow--2{flex-grow:2}.flex-grow--3{flex-grow:3}table{border-collapse:collapse;border-color:rgba(34,34,34,0.05);border-spacing:.75em;width:100%}td,th{font-weight:normal;padding:.75em;text-align:left}td:first-child,th:first-child{padding-left:2em}td:last-child,th:last-child{padding-right:2em}tr{border-bottom:1px solid rgba(34,34,34,0.1);border-color:rgba(34,34,34,0.05);transition:background .2s}tr:last-child{border-bottom:none}tr:hover{background-color:rgba(34,34,34,0.03)}thead>tr{background-color:transparent !important;border-color:rgba(34,34,34,0.05)}thead>tr:last-child{border-bottom:1px solid rgba(34,34,34,0.1);border-color:rgba(34,34,34,0.05)}caption{padding:.75em 2em}tr.selected{background-color:rgba(34,34,34,0.05)}.container{background-color:#fff;border-radius:.313rem;box-shadow:0 0.22rem 0.5rem rgba(34,34,34,0.03);padding:.25em 0}.sidebar{margin-right:2em;min-width:20em}.jobs-column--args{font-family:"SFMono-Regular","Consolas","Liberation Mono","Menlo","Courier",monospace}.menu ul,.menu li{margin-bottom:0}.menu--contained{background-color:#fff;border-radius:.313rem;box-shadow:0 0.22rem 0.5rem rgba(34,34,34,0.03);padding:.25em 0}.menu-list{list-style:none;padding:0}.menu-title{font-size:1em;font-weight:700;margin:0;padding:0.75em 1em}.menu-divider{border-top:1px solid #e9e9e9;margin:.25em 0;padding:0}.menu-item{color:inherit;cursor:pointer;display:block;margin-bottom:0;padding:0.75em 1em;position:relative;transition:background .2s}.menu-item:hover{background-color:rgba(34,34,34,0.03)}.menu-item--active:before{background-color:#1d83f6;content:'';display:block;height:100%;left:0;position:absolute;top:0px;width:2px}.menu-item--selected{background-color:rgba(34,34,34,0.03)}.menu-item,.menu-toggle{appearance:none;background-color:transparent;font-family:inherit;font-size:inherit;line-height:1.5;position:relative;text-align:left;width:100%}.menu-item:active,.menu-toggle:active{color:inherit}.menu-item[disabled],.menu-item.disabled,.menu-toggle[disabled],.menu-toggle.disabled{opacity:0.3;pointer-events:none}
